#include<iostream>
#include<cmath>
using namespace std;
int main(){
int x;
cout<<"enter the number"<<endl;
cin>>x;
if (x%2==0)
cout<<"it is divisible by 2"<<endl;
else if (x%3==0)
    cout<<"it is divisible by 3"<<endl;
else if (x%5==0)
    cout<<"it is divisible by 5"<<endl;
else if (x%7==0)
    cout<<"it is divisible by 7
    "<<endl;

return 0;






}
