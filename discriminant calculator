#include<iostream>
#include<cmath>
using namespace std;
int main(){
int x,a,c,b,d,y;

cout<<"enter the value of a"<<endl;
cin>>a;
cout<<"enter the value of c"<<endl;
cin>>c;
cout<<"enter the value of b"<<endl;
cin>>b;
x=pow(b,2);
d=x-(4*a*c);
y=sqrt (d);
if (d=0)
{
    cout<<"there is only 1 root : "<<-b/(2*a)<<endl;
}
else if(d>0)
{
    cout<<"there are two real roots : "<<(-b+y)/2*a<<endl;
    cout<<(-b-y)/2*a<<endl;
}
else
    cout<<"there are no real roots"<<endl;
return 0;
}
