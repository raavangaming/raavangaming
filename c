#include<iostream>
#include<cmath>
using namespace std;
int main(){
double a;
double b;
double c;
double m;
double y;
double z;
double t;
double x;
double r;
cout<<"the value of b"<<endl;
cin>>b;
cout<<"the value of c"<<endl;
cin>>c;
cout<<"the value of a"<<endl;
cin>>a;
m=pow(b,2);
r=2*a;
y=4*a*c;
z=abs(m-y);
t=-b+z;

x=t/r;
cout<<"the value of x is :"<< x<<endl;
return 0;






}
