#include<iostream>
#include<cmath>
using namespace std;
int main(){
int x;
cout<<"please enter your marks"<<endl;
cin>>x;
if (x<=0||x>=100)
    cout<<"it is invalid"<<endl;
else if (x>=85)
cout<<"your grade is A"<<endl;
else if (x>=80)
cout<<"your grade is A-"<<endl;
else if (x>=75)
cout<<"your grade is B+"<<endl;
else if (x>=70)
cout<<"your grade is B"<<endl;
else if (x>=65)
cout<<"your grade is B-"<<endl;
else if (x>=60)
cout<<"your grade is C+"<<endl;
else if (x>=55)
cout<<"your grade is C"<<endl;
else if (x>=50)
cout<<"your grade is C-"<<endl;
else if (x>=45)
cout<<"your grade is D+"<<endl;
else if (x>=40)
cout<<"your grade is D"<<endl;
else cout<<"You failed"<<endl;













}
